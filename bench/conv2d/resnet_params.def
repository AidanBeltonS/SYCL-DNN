/**
 * \file
 * X-Macro definiton file for VGG convolution sizes.
 *
 * Contains a number of calls to the RESNET_PARAMS function macro defining
 * the following convolution parameters, as used in the Resnet network.
 *
 * The ordering of the arguments is:
 * \code
 *   RESNET_PARAMS(Channels, Width, Height, Filter, Stride, Features)
 * \endcode
 *
 * Channels | Width | Height | Filter | Stride | Features
 * ---------|-------|--------|--------|--------|---------
 *        3 |   230 |    230 |      7 |      2 |       64
 *       64 |    56 |     56 |      1 |      1 |      256
 *       64 |    56 |     56 |      1 |      1 |       64
 *       64 |    56 |     56 |      3 |      1 |       64
 *      256 |    56 |     56 |      1 |      1 |       64
 *      256 |    56 |     56 |      1 |      2 |      512
 *      256 |    56 |     56 |      1 |      2 |      128
 *      128 |    28 |     28 |      3 |      1 |      128
 *      128 |    28 |     28 |      1 |      1 |      512
 *      512 |    28 |     28 |      1 |      1 |      128
 *      512 |    28 |     28 |      1 |      1 |      128
 *      512 |    28 |     28 |      1 |      2 |     1024
 *      512 |    28 |     28 |      1 |      2 |      256
 *      256 |    14 |     14 |      3 |      1 |      256
 *      256 |    14 |     14 |      1 |      1 |     1024
 *     1024 |    14 |     14 |      1 |      1 |      256
 *     1024 |    14 |     14 |      1 |      2 |     2048
 *     1024 |    14 |     14 |      1 |      2 |      512
 *      512 |     7 |      7 |      3 |      1 |      512
 *      512 |     7 |      7 |      1 |      1 |     2048
 *     2048 |     7 |      7 |      1 |      1 |      512
 */
#ifndef RESNET_PARAMS
#error This file expects the RESNET_PARAMS macro to be defined.
#endif

RESNET_PARAMS(3, 230, 230, 7, 2, 64)
RESNET_PARAMS(64, 56, 56, 1, 1, 256)
RESNET_PARAMS(64, 56, 56, 1, 1, 64)
RESNET_PARAMS(64, 56, 56, 3, 1, 64)
RESNET_PARAMS(256, 56, 56, 1, 1, 64)
RESNET_PARAMS(256, 56, 56, 1, 2, 512)
RESNET_PARAMS(256, 56, 56, 1, 2, 128)
RESNET_PARAMS(128, 28, 28, 3, 1, 128)
RESNET_PARAMS(128, 28, 28, 1, 1, 512)
RESNET_PARAMS(512, 28, 28, 1, 1, 128)
RESNET_PARAMS(512, 28, 28, 1, 2, 1024)
RESNET_PARAMS(512, 28, 28, 1, 2, 256)
RESNET_PARAMS(256, 14, 14, 3, 1, 256)
RESNET_PARAMS(256, 14, 14, 1, 1, 1024)
RESNET_PARAMS(1024, 14, 14, 1, 1, 256)
RESNET_PARAMS(1024, 14, 14, 1, 2, 2048)
RESNET_PARAMS(1024, 14, 14, 1, 2, 512)
RESNET_PARAMS(512, 7, 7, 3, 1, 512)
RESNET_PARAMS(512, 7, 7, 1, 1, 2048)
RESNET_PARAMS(2048, 7, 7, 1, 1, 512)
