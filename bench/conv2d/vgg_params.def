/**
 * \file
 * X-Macro definiton file for VGG convolution sizes.
 *
 * Contains a number of calls to the VGG_PARAMS function-like macro defining
 * the following convolution parameters, as used in the VGG network.
 *
 * The ordering of the arguments is:
 * \code
 *   VGG_PARAMS(Channels, Width, Height, Features)
 * \endcode
 * The convolutions filters are all 3x3, and the strides are all 1.
 *
 * Channels | Width | Height | Filter | Stride | Features
 * ---------|-------|--------|--------|--------|---------
 *        3 |   224 |    224 |      3 |      1 |       64
 *       64 |   224 |    224 |      3 |      1 |       64
 *       64 |   112 |    112 |      3 |      1 |      128
 *      128 |   112 |    112 |      3 |      1 |      128
 *      128 |    56 |     56 |      3 |      1 |      256
 *      256 |    56 |     56 |      3 |      1 |      256
 *      256 |    28 |     28 |      3 |      1 |      512
 *      512 |    28 |     28 |      3 |      1 |      512
 *      512 |    14 |     14 |      3 |      1 |      512
 */
#ifndef VGG_PARAMS
#error This file expects the VGG_PARAMS macro to be defined.
#endif

VGG_PARAMS(3, 224, 224, 64)
VGG_PARAMS(64, 224, 224, 64)
VGG_PARAMS(64, 112, 112, 128)
VGG_PARAMS(128, 112, 112, 128)
VGG_PARAMS(128, 56, 56, 256)
VGG_PARAMS(256, 56, 56, 256)
VGG_PARAMS(256, 28, 28, 512)
VGG_PARAMS(512, 28, 28, 512)
VGG_PARAMS(512, 14, 14, 512)
